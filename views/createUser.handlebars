 <section>
        <div class="container">
            <div class="card-panel z-depth-1" id="enterinfo">
                <h4 class="center">New User</h4>
                <br><br>
                <form class="container col s6 center">
                    <div class="input-field">
                        <input id="first_name" type="text" class="validate">
                        <label for="first_name">First Name:</label>
                    </div>
                    <div class="input-field">
                        <input id="last_name" type="text" class="validate">
                        <label for="last_name">Last Name:</label>
                    </div>
                    <div class="input-field ">
                        <input id="userName" type="text" class="validate">
                        <label for="username"> Username: </label>
                    </div>
                    <div class="input-field center-align">
                        <input id="password" type="password" data-rule-password="true" class="validate">
                        <label for="password">Password:</label>
                    </div>
                    <div class="input-field">
                        <input id="password_confirm" type="password" data-rule-password="true" data-rule-equalTo="#password" class="validate">
                        <label for="password_confirm">Confirm Password:</label>
                    </div>
                    <div>
                        <button class="btn indigo lighten-2 waves-effect waves-light" id="submitUserInfo">Create User
                </button>

                </form>
                
            </div>
        </div>
    </section>

    <script src="addUser.js"></script>