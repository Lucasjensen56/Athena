

<main>
  <div id="logInSection" class="container center-align z-depth-2">
      <h3>Log in</h3>
      <div class="row">
        <form class="col s12"  style="backgroud-color: white;" id="formLogin">
          <div class="input-field">
            <input type="email" id="email" class="validate" placeholder="Email">
            <label for="email" id="emailText"></label>
          </div>

          <div class="input-field">
            <input type="password" id="password" class="validate" placeholder="Password">
            <label for="password" id="pwText"></label>
          </div>

          <br>

          <div class="row">
            <div class="col s12">



              <button class="#7986cb indigo lighten-2 btn col s12" id="submit">Log In</button>

            </div>
          </div>
        </form>

      </div>

      <div class="row">
        <div class="col s12">
          <button class="#7986cb indigo lighten-2 btn btn-small"><a href="/createUser">If new user, create account</a></button>
        </div>
      </div>

  </div>
</main>
      <script src="login.js"></script>

</main>
